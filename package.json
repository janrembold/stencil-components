{
  "name": "stencil-components",
  "version": "0.0.3",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "storybook.start": "npm-run-all --parallel storybook build.watch",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@pxtrn/storybook-addon-docs-stencil": "^1.0.0",
    "@rollup/plugin-commonjs": "17.0.0",
    "@stencil/postcss": "2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "1.4.1",
    "@storybook/addon-actions": "6.1.15",
    "@storybook/addon-essentials": "6.1.15",
    "@storybook/addon-links": "6.1.15",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/web-components": "6.1.15",
    "@types/autoprefixer": "9.7.2",
    "@types/jest": "^26.0.20",
    "autoprefixer": "10.2.3",
    "babel-loader": "8.2.2",
    "lit-html": "1.3.0",
    "npm-run-all": "4.1.5",
    "storybook-addon-themes": "^6.0.1",
    "typescript": "4.1.3"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ]
}
