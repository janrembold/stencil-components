{
  "name": "stencil-components",
  "version": "0.0.3",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-components/stencil-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch",
    "clean": "npm-run-all clean.*",
    "clean.dist": "rimraf dist",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "start": "stencil build --dev --watch --serve",
    "storybook": "start-storybook -p 6006",
    "storybook.build": "build-storybook",
    "storybook.start": "npm-run-all --parallel storybook build.watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/core": "2.4.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@pxtrn/storybook-addon-docs-stencil": "1.0.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/postcss": "2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "1.4.1",
    "@storybook/addon-actions": "6.1.16",
    "@storybook/addon-essentials": "6.1.16",
    "@storybook/web-components": "6.1.16",
    "@types/autoprefixer": "10.2.0",
    "@types/jest": "26.0.20",
    "@types/puppeteer": "5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "10.2.4",
    "babel-loader": "8.2.2",
    "eslint": "^7.19.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lit-html": "1.3.0",
    "npm-run-all": "4.1.5",
    "puppeteer": "6.0.0",
    "rimraf": "3.0.2",
    "storybook-addon-themes": "6.0.1",
    "typescript": "4.1.3"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ]
}
