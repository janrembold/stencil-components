    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/sass": "^1.4.1",
    "@types/autoprefixer": "^9.7.2",
    "autoprefixer": "^10.2.3"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 11"
  ]
}
